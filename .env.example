# Environment Configuration
# Copy this file to .env and customize as needed

# n8n Configuration
N8N_HOST=local.n8n.insta.com
N8N_PORT=5678
N8N_PROTOCOL=https
NODE_ENV=production
WEBHOOK_URL=https://local.n8n.insta.com/
GENERIC_TIMEZONE=Asia/Kolkata

# Nginx Configuration
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443

# PostgreSQL 17 Configuration
POSTGRES_VERSION=17
POSTGRES_DB=n8n_db
POSTGRES_USER=n8n_user
POSTGRES_PASSWORD=n8n_secure_password_change_me
POSTGRES_PORT=5432

# PostgreSQL Additional Configuration
# Create additional databases (comma-separated)
ADDITIONAL_DATABASES=

# Create additional users (format: username:password:database, comma-separated)
# Example: ADDITIONAL_USERS=user1:pass1:db1,user2:pass2:db2
ADDITIONAL_USERS=

# Enable pgvector extension (true/false)
ENABLE_PGVECTOR=true

# PostgreSQL Timezone
POSTGRES_TIMEZONE=UTC

# Add your custom environment variables below
